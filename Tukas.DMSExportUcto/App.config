<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="exportSettings" type="Tukas.DMSExportUcto.Config.ExportConfigurationSection, Tukas.DMSExportUcto"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <connectionStrings>
    <add name="dataConnection" connectionString="Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=tcp)(HOST=localhost)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=ORCL)));User Id=myUsername;Password=myPassword;"/>
  </connectionStrings>
  <exportSettings 
